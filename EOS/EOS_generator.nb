 RowBox[{
  RowBox[{
   RowBox[{"SciNot", "[", "num_", "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"exp", "=", 
        RowBox[{"Floor", "[", 
         RowBox[{"Log10", "[", "num", "]"}], "]"}]}], ",", " ", 
       RowBox[{"dig", "=", "0"}], ",", "  ", 
       RowBox[{"remain", "=", "0"}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"dig", "=", 
       RowBox[{"Floor", "[", 
        RowBox[{"num", "/", 
         SuperscriptBox["10", "exp"]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"remain", "=", 
       RowBox[{
        RowBox[{"num", "/", 
         SuperscriptBox["10", "exp"]}], "-", "dig"}]}], " ", ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", " ", "[", 
       RowBox[{
        RowBox[{"remain", "\[Equal]", " ", "0"}], ",", "\[IndentingNewLine]", 
        
        RowBox[{"Return", "[", 
         RowBox[{
          RowBox[{"ToString", "[", "dig", "]"}], "<>", "\"\<e\>\"", "<>", 
          RowBox[{"ToString", "[", "exp", "]"}]}], "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"Return", "[", 
         RowBox[{
          RowBox[{"ToString", "[", "dig", "]"}], "<>", "\"\<.\>\"", "<>", 
          RowBox[{
           RowBox[{"StringSplit", "[", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"ToString", "[", "remain", "]"}], ",", "\"\<.\>\""}], 
            "]"}], "\[LeftDoubleBracket]", 
           RowBox[{"-", "1"}], "\[RightDoubleBracket]"}], 
          "\[IndentingNewLine]", "<>", "\"\<e\>\"", "<>", 
          RowBox[{"ToString", "[", "exp", "]"}]}], "]"}]}], 
       "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Concat", "[", 
   RowBox[{"strings_", ",", "nums_"}], "]"}], ":=", 
  RowBox[{"StringJoin", "@", 
